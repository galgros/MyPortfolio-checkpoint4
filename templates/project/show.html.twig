        <h1>Projet</h1>
    </div>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ project.id }}</td>
        </tr>
        <tr>
            <th>Titre</th>
            <td>{{ project.title }}</td>
        </tr>
        <tr>
            <th>DÃ©scription</th>
