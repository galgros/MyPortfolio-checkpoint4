<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Galgros-MrSample-Samplito{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        {{ encore_entry_link_tags('app') }}
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-dark">
            <a class="navbar-brand" href="/">
                <div>Galgros</div>
                <div><small>Mr Sample</small></div>
                <div><small>Samplito</small></div>
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item active">
                        <a class="nav-link" href="{{ path('app_dev') }}">Dev</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="{{ path('app_music') }}">Music</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="{{ path('app_gaming') }}">Gaming</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="{{ path('app_video') }}">Video</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="{{ path('app_picture') }}">Picture</a>
                    </li>
                </ul>
            </div>
        </nav>
        {% block body %}{% endblock %}
        <footer>Footer section</footer>
        {% block javascripts %}{% endblock %}
        {{ encore_entry_script_tags('app') }}
    </body>
</html>
